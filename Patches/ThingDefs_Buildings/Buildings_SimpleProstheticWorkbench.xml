<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ========== Simple Prosthetics Workbench ========== -->
	<Operation Class="PatchOperationReplace">
		<xpath>*/ThingDef[DefName="TableSimpleProsthetic"]/CostList/Steel</xpath>
		<value>
			<Steel>120</Steel>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>*/ThingDef[DefName="TableSimpleProsthetic"]/CostList/Component</xpath>
		<value>
			<Component>5</Component>
		</value>
	</Operation>
	<Operation Class="PatchOperationRemove">
		<xpath>*/ThingDef[DefName="TableSimpleProsthetic"]/recipes/li[10]</xpath> <!-- removes golden eye recipe -->
	</Operation>

</Patch>