<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ========== Basic Prosthetic Workbench ========== -->
	<Operation Class="PatchOperationReplace">
		<xpath>*/ThingDef[DefName="TableBasicProsthetic"]/costStuffCount</xpath> <!-- Ykara plz. 'DefName' instead of 'defName' initially tripped me up :P -->
		<value>
			<costStuffCount>60</costStuffCount>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>*/ThingDef[DefName="TableBasicProsthetic"]/CostList/Steel</xpath>
		<value>
			<Steel>20</Steel>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>*/ThingDef[DefName="TableBasicProsthetic"]/statBases/WorkToMake</xpath>
		<value>
			<WorkToBuild>2000</WorkToBuild> <!-- and nobody reports this issue in base EPOE... no more instant workbenches, muahahaha! -->
		</value>
	</Operation>

</Patch>