<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ========== Basic Prosthetic Workbench ========== -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="TableBasicProsthetic"]/costStuffCount</xpath> 
		<value>
			<costStuffCount>60</costStuffCount>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="TableBasicProsthetic"]/costList/Steel</xpath>
		<value>
			<Steel>20</Steel>
		</value>
	</Operation>
<!-- Fixed in EPOE 2.1	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="TableBasicProsthetic"]/statBases/WorkToMake</xpath>
		<value>
			<WorkToBuild>2000</WorkToBuild>
		</value>
	</Operation> -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="TableBasicProsthetic"]</xpath>
		<value>
			<researchPrerequisites>
				<li>BasicProsthetics</li>
			</researchPrerequisites>
		</value>
	</Operation>

</Patch>