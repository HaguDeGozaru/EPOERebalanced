<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ========== Mince Organs ========== -->
	<Operation Class="PatchOperationReplace">
		<xpath>*/RecipeDef[defName="MinceOrgans"]/products/Human_Meat</xpath>
		<value>
			<Human_Meat>2</Human_Meat> <!-- Organs average ~2% coverage, or 1.8 meat -->
		</value>
	</Operation>

	<!-- ========== Mince Limbs ========== -->
	<Operation Class="PatchOperationReplace">
		<xpath>*/RecipeDef[defName="MinceLimbs"]/products/Human_Meat</xpath>
		<value>
			<Human_Meat>6</Human_Meat> <!-- Limbs average ~7% coverage, or 6.3 meat -->
		</value>
	</Operation>

</Patch>