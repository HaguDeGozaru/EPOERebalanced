<?xml version="1.0" encoding="UTF-8"?>
<Patch>

   <!-- ========== AI Chip ========== -->
   <Operation Class="PatchOperationAdd">
		<xpath>Defs/HediffDef[defName="AIChip"]/stages/li</xpath>
		<value>
			<partIgnoreMissingHP>true</partIgnoreMissingHP>
		</value>
	</Operation>
	<Operation Class="PatchOperationRemove">
		<xpath>Defs/HediffDef[defName="AIChip"]/stages/li/capMods</xpath>
	</Operation>

	<!-- ========== AI Persona Core ========== -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/HediffDef[defName="AIPersonaCore"]/addedPartProps/partEfficiency</xpath>
		<value>
			<partEfficiency>1.2</partEfficiency>
		</value>
	</Operation>
	<Operation Class="PatchOperationRemove">
		<xpath>Defs/HediffDef[defName="AIPersonaCore"]/stages/li/capMods/li[1]</xpath>
	</Operation>
	<Operation Class="PatchOperationRemove">
		<xpath>Defs/HediffDef[defName="AIPersonaCore"]/stages/li/capMods/li[1]</xpath>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/HediffDef[defName="AIPersonaCore"]/stages/li/capMods/li/offset</xpath>
		<value>
			<offset>0.1</offset>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/HediffDef[defName="AIPersonaCore"]/stages/li/mentalStateGivers</xpath>
		<value>
			<mentalStateGivers>
				<li>
					<mentalState>Berserk</mentalState>
					<mtbDays>14</mtbDays>
				</li>
				<li>
					<mentalState>WanderPsychotic</mentalState>
					<mtbDays>8</mtbDays>
				</li>
			</mentalStateGivers>
		</value>
	</Operation>

