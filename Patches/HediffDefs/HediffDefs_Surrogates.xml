<?xml version="1.0" encoding="UTF-8"?>
<Patch>

   <!-- ========== Brain Stimulator ========== -->
	<Operation Class="PatchOperationAdd">
		<xpath>*/HediffDef[defName="BrainStimulator"]/stages/li</xpath>
		<value>
			<partIgnoreMissingHP>true</partIgnoreMissingHP>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>*/HediffDef[defName="BrainStimulator"]/stages/li/capMods</xpath>
		<value>
			<capMods>
				<li>
					<capacity>Consciousness</capacity>
					<postFactor>0.65</postFactor>
				</li>
			</capMods>
		</value>
	</Operation>

	<!-- ========== Surrogate Lung ========== -->
	<Operation Class="PatchOperationRemove">
		<xpath>*/HediffDef[defName="SurrogateLung"]/stages</xpath>
	</Operation>

</Patch>