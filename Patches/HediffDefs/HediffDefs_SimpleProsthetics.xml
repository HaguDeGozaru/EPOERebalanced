<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ========== Golden Eye ========== -->
	<Operation Class="PatchOperationRemove">
		<xpath>*/HediffDef[defName="GoldenEye"]</xpath>
	</Operation>

	<!-- ========== Simple Prosthetic Hand ========== -->
	<Operation Class="PatchOperationRemove">
		<xpath>*/HediffDef[defName="SimpleProstheticHand"]/comps</xpath>
	</Operation>

	<!-- ========== Simple Prosthetic Arm ========== -->
	<Operation Class="PatchOperationRemove">
		<xpath>*/HediffDef[defName="SimpleProstheticArm"]/comps</xpath>
	</Operation>

	<!-- ========== Hydraulic Jaw ========== -->
	<Operation Class="PatchOperationRemove">
		<xpath>*/HediffDef[defName="HydraulicJaw"]/stages</xpath>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>*/HediffDef[defName="HydraulicJaw"]/addedPartProps/partEfficiency</xpath>
		<value>
			<partEfficiency>0.9</partEfficiency>
		</value>
	</Operation>

</Patch>